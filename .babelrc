{
    "presets": [
      ["env", {
        "targets": {
          "browsers": [
            "Chrome >= 52",
            "FireFox >= 44",
            "Safari >= 7",
            "Explorer 11",
            "last 4 Edge versions"
          ],
          "node": "current"
        },
        "modules": false,
      }],
      "stage-1"
    ],
    "ignore": [
      "node_modules"
    ],
    "plugins": [
      "external-helpers",
      "transform-decorators-legacy",
      "transform-class-properties",
      "syntax-class-properties"
    ]
  }